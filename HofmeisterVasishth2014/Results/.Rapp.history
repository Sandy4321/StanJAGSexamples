univ.stan <- 'data {#
    int<lower=1> N;#
    real logRTresidual[N];			// outcome#
    real f1[N];					// predictor#
    real f2[N];					// predictor#
    real f1_X_f2[N];				// interaction#
    real spill1[N];#
    real spill2[N];#
    real corr[N];					// predictor#
    real f1_X_corr[N];					// predictor#
    real f2_X_corr[N];				// interaction#
    real f1_X_f2_X_corr[N];				// interaction#
#
    int<lower=1> I;   				// number of subjects#
    int<lower=1> J;				// number of items#
    int<lower=1,upper=I> subject_id[N];	// subjects#
    int<lower=1,upper=J> item_id[N];	// items#
    vector[10] subj_prior; 			//vector of zeros passed in from R#
    vector[4] item_prior; 			//vector of zeros passed in from R#
#
}#
#
transformed data{#
   real ZERO; // like #define ZERO 0 in C/C++   ZERO <- 0.0;#
}#
#
parameters{#
    real Intercept;#
    real beta_f1;#
    real beta_f2;#
    real beta_f1_X_f2;#
    real beta_spill1;#
    real beta_spill2;#
    real beta_corr;#
    real beta_f1_X_corr;#
    real beta_f2_X_corr;#
    real beta_f1_X_f2_X_corr;#
    real<lower=0,upper=10> sigma_e;						// residual sd#
    vector[10] u[I];							// random intercept for subjs & 3 slopes for f1, f2, & f12#
    vector<lower=0,upper=10>[10] sigma_u;						// subject sds #
    corr_matrix[10] Omega_subj;					// corr matrix for subjs#
    vector[4] w[J];							// random intercept for items & 3 slopes for f1, f2, & f12#
    vector<lower=0,upper=10>[4] sigma_w;						// item sds#
    corr_matrix[4] Omega_item;					// corr matrix for items#
}#
#
transformed parameters {#
	cov_matrix[10] Sigma_subj;	cov_matrix[4] Sigma_item;#
#
    	Sigma_subj <- diag_matrix(sigma_u) * Omega_subj * diag_matrix(sigma_u);#
	Sigma_item <- diag_matrix(sigma_w) * Omega_item * diag_matrix(sigma_w);}#
#
model{#
    	real mu[N];#
	matrix[10,10] L;	matrix[10,10] DL;#
	matrix[4,4] M;	matrix[4,4] DM;#
#
   	 // Priors#
	Intercept ~ normal(0,10);#
    	beta_f1 ~ normal( 0 , 10 );#
	beta_f1 ~ normal( 0 , 10 );#
    	beta_f1_X_f2 ~ normal( 0 , 10 );#
	spill1 ~ normal(0, 10);#
	spill2 ~ normal(0, 10);#
	beta_corr ~ normal(0, 10); #
    	beta_f1_X_corr ~ normal( 0 , 10 );#
	beta_f2_X_corr ~ normal( 0 , 10 );#
    	beta_f1_X_f2_X_corr ~ normal( 0 , 10 );#
    	sigma_e ~ uniform( 0 , 10 );			// residuals#
    	sigma_u ~ uniform( 0 , 10 );			// subj sd#
    	sigma_w ~ uniform( 0 , 10 );			// item sd#
#
    	Omega_subj ~ lkj_corr(4.0);#
    	Omega_item ~ lkj_corr(4.0);#
#
	L <- cholesky_decompose(Omega_subj);#
	M <- cholesky_decompose(Omega_item);#
	for (m in 1:10)		for (n in 1:m)			DL[m,n] <- L[m,n] * sigma_u[m];	for (m in 1:10)		for (n in (m+1):10)			DL[m,n] <- ZERO;#
	for (m in 1:4)		for (n in 1:m)			DM[m,n] <- M[m,n] * sigma_w[m];	for (m in 1:4)		for (n in (m+1):4)			DM[m,n] <- ZERO;#
#
    // Varying effects#
    for ( i in 1:I ) u[i] ~ multi_normal_cholesky( subj_prior, DL );#
    for ( j in 1:J ) w[j] ~ multi_normal_cholesky( item_prior, DM );#
#
    // Fixed effects#
    for ( i in 1:N ) {#
        mu[i] <- 	Intercept + #
			beta_f1*f1[i] + #
			beta_f2*f2[i] + #
			beta_f1_X_f2*f1_X_f2[i] + #
			beta_spill1*spill1[i] + #
			beta_spill2*spill2[i] + #
			beta_corr*corr[i] +  #
			beta_f1_X_corr*f1_X_corr[i] + #
			beta_f2_X_corr*f2_X_corr[i] + #
			beta_f1_X_f2_X_corr*f1_X_f2_X_corr[i] + #
#
			u[subject_id[i],1] + #
			u[subject_id[i],2]*f1[i] + #
			u[subject_id[i],3]*f2[i] + #
			u[subject_id[i],4]*f1_X_f2[i] + #
			u[subject_id[i],5]*spill1[i] + #
			u[subject_id[i],6]*spill2[i] + #
#
			u[subject_id[i],7]*corr[i] + #
			u[subject_id[i],8]*f1_X_corr[i] + #
			u[subject_id[i],9]*f2_X_corr[i] + #
			u[subject_id[i],10]*f1_X_f2_X_corr[i] + #
			w[item_id[i],1] + #
			w[item_id[i],2]*f1[i] + #
			w[item_id[i],3]*f2[i] + #
			w[item_id[i],4]*f1_X_f2[i];#
    }#
    logRTresidual ~ normal( mu , sigma_e );#
}'
