pos <- function(dat,  subj.name='subj',item.name='item',expt.name='expt') {#
    dat <- data.frame(subj = dat[[subj.name]], item = dat[[item.name]],expt=dat[[expt.name]])#
    k <- 0    ## subj number#
    m <- 0    ## item number#
    ipos <- c() #
    for(i in 1:nrow(dat)) {#
    	   if (dat$subj[i] != k) #
	   # if the subject number is different than the last start counter over#
	      	 {#
		  k <- dat$subj[i]#
		 counter <- 1#
		  ipos[i] <- counter#
		  }#
          else if ((dat$item[i] != dat$item[i-1]) || (dat$expt[i] != dat$expt[i-1]) ) {#
	   ## if either the item number or expt name is different,#
	   ####    	   increment the counter#
            m <- dat$item[i]#
            counter <- counter + 1#
	    ipos[i] <- counter#
	         }#
	  else #
	       {#
                 ipos[i] <- counter#
	      } #
}#
    return(ipos)#
}#
